{
    "parser": "@typescript-eslint/parser",
    "plugins": ["svelte3", "@typescript-eslint"],
    "extends": ["../../.eslintrc.json"],
    "ignorePatterns": ["!**/*", "vite.config.ts"],
    "overrides": [
        {
            "files": ["*.ts", "*.js", "*.svelte"],
            "rules": {}
        },
        {
            "files": ["*.ts", "*.tsx"],
            "rules": {}
        },
        {
            "files": ["*.js", "*.jsx"],
            "rules": {}
        },
        {
            "files": ["*.svelte"],
            "processor": "svelte3/svelte3"
        }
    ]
}
