{
    "name": "options-e2e",
    "$schema": "../../node_modules/nx/schemas/project-schema.json",
    "projectType": "application",
    "sourceRoot": "apps/options-e2e/src",
    "tags": [],
    "implicitDependencies": ["options"],
    "targets": {
        "e2e": {
            "executor": "@nx/cypress:cypress",
            "options": {
                "cypressConfig": "apps/options-e2e/cypress.config.ts",
                "testingType": "e2e",
                "devServerTarget": "options:serve",
                "port": 5000
            },
            "configurations": {
                "production": {
                    "devServerTarget": "options:serve:production"
                },
                "ci": {
                    "devServerTarget": "options:serve-static"
                }
            }
        },
        "lint": {
            "executor": "@nx/eslint:lint"
        }
    }
}
